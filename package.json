{
  "name": "react-petfinder",
  "version": "1.0.0",
  "description": "Simple React demo app",
  "main": "src/App.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "format-check": "prettier --list-different \"src/**/*.{js,jsx,css,json}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "test": "jest --silent",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --no-cache"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!petfinder-client).+"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rlewis2892/react-petfinder.git"
  },
  "keywords": [],
  "author": "Rochelle Lewis",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rlewis2892/react-petfinder/issues"
  },
  "homepage": "https://github.com/rlewis2892/react-petfinder#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.1.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.2"
  },
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@reach/router": "^1.2.1",
    "petfinder-client": "0.0.3",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
